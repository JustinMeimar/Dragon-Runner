{
  "testDir": "testfiles",
  "testedExecutablePaths": {
    "solution": "../bin/gazc"
  },
  "solutionExecutable": "solution",
  "runtimes": {
    "solution": "../bin/libgazrt.so"
  },
  "toolchains": {
    "gazprea-llc": [
      {
        "stepName": "gazprea",
        "executablePath": "$EXE",
        "arguments": ["$INPUT", "$OUTPUT"],
        "output": "/tmp/gaz.ll",
        "allowError": true 
      }, 
      {
        "stepName": "llc",
        "executablePath": "/home/justin/install/llvm/llvm-18/bin/llc",
        "arguments": ["-filetype=obj", "-relocation-model=pic", "$INPUT", "-o", "$OUTPUT"],
        "output": "/tmp/gaz.o"
      },
      {
        "stepName": "clang",
        "executablePath": "/usr/bin/clang",
        "arguments": ["$INPUT", "-o", "$OUTPUT", "-L$RT_PATH", "-l$RT_LIB", "-lm"],
        "output": "/tmp/gaz"
      },
      {
        "stepName": "run",
        "executablePath": "$INPUT",
        "arguments": [],
        "usesInStr": true,
        "usesRuntime": true,
        "allowError": true
      }
    ] 
  }
}
